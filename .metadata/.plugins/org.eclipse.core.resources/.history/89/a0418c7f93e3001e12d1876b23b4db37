	        int answer = 0;
	        int N = triangle.length; //5
	        int [][] dp = new int[N+1][N+1];
	        
	        
	        
	        return answer;
	    }

	public static void main(String[] args) {
		// 1,2,3,4,5
		solution(new int [][] {{7},{3,8},{8,1,0},{2,7,4,4},{4,5,2,6,5}});

